{
  "name": "@supaglue/grpc",
  "private": true,
  "version": "0.2.0",
  "dependencies": {
    "@grpc/grpc-js": "^1.8.8",
    "google-protobuf": "^3.21.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "typescript": "^4.9.4"
  },
  "exports": {
    "./grpc-js": "./src/grpc-js.ts",
    "./*": {
      "types": "./gen/*.d.ts",
      "require": "./gen/*.js"
    }
  },
  "scripts": {
    "build": "yarn run clean && tsc -p .",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "lint-staged": "run -T lint-staged --no-stash --quiet",
    "lint": "run -T eslint"
  },
  "publishConfig": {
    "exports": {
      "./grpc-js": {
        "types": "./dist/grpc-js.d.ts",
        "require": "./dist/grpc-js.js"
      },
      "./*": {
        "types": "./gen/*.d.ts",
        "require": "./gen/*.js"
      }
    }
  }
}
