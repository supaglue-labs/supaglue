{
  "name": "@supaglue/cli",
  "version": "0.1.1-1",
  "repository": {
    "type": "git",
    "url": "https://github.com/supaglue-labs/supaglue.git",
    "directory": "apps/cli"
  },
  "dependencies": {
    "@sentry/integrations": "^7.34.0",
    "@sentry/node": "^7.34.0",
    "@supaglue/sdk": "workspace:*",
    "axios": "^1.2.2",
    "chalk": "^4.1.2",
    "clui": "^0.3.6",
    "cron-parser": "^4.7.1",
    "diff": "^5.1.0",
    "global-dirs": "^3.0.1",
    "posthog-node": "^2.2.3",
    "string-width": "4.2.3",
    "table": "^6.8.1",
    "tslib": "^2.5.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "update-check": "^1.5.4",
    "uuid": "^9.0.0",
    "yargs": "^17.6.2",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.4.1",
    "@types/clui": "^0",
    "@types/diff": "^5",
    "@types/uuid": "^9",
    "@types/yargs": "^17",
    "@types/yargs-parser": "^21"
  },
  "bin": {
    "supaglue": "./src/dev-cli.js"
  },
  "publishConfig": {
    "bin": {
      "supaglue": "./dist/runner.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run -T tsc -p tsconfig.build.json",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "publish": "run -T turbo run build --filter=cli... && yarn npm publish",
    "lint-staged": "run -T lint-staged --no-stash",
    "lint": "run -T eslint"
  }
}
