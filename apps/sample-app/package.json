{
  "name": "sample-app",
  "version": "0.1.1-1",
  "private": true,
  "scripts": {
    "dev": "prisma migrate deploy && prisma db seed && next dev",
    "build": "sh -c 'if [ \"$VERCEL_ENV\" = production ]; then yarn prisma migrate deploy; fi; next build'",
    "start": "next start",
    "lint": "next lint",
    "lint-staged": "run -T lint-staged --no-stash --quiet"
  },
  "peerDependencies": {
    "prettier": "*"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@next/env": "13.1.1",
    "@next/font": "13.1.1",
    "@prisma/client": "^4.10.1",
    "@supaglue/nextjs": "workspace:*",
    "@supaglue/sdk": "workspace:*",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "daisyui": "^2.49.0",
    "dotenv": "^16.0.3",
    "eslint": "8.30.0",
    "eslint-config-next": "13.0.7",
    "jsforce": "^1.11.0",
    "next": "^13.1.1",
    "next-auth": "^4.18.6",
    "nodemailer": "^6.8.0",
    "p-queue": "^7.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.31.3",
    "swr": "^2.0.0",
    "tailwind-override": "^0.6.1",
    "tsx": "^3.12.2"
  },
  "devDependencies": {
    "@tsconfig/next": "^1.0.4",
    "@types/bcryptjs": "^2",
    "@types/jsforce": "^1.11.0",
    "@types/nodemailer": "^6",
    "@types/sharp": "^0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.10.1",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "tsc-files --noEmit next-env.d.ts",
    "**/*.{ts,tsx,js}": [
      "eslint",
      "prettier --write"
    ]
  }
}
